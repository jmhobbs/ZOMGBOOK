{
  "name": "ZOMGBOOK!",
	"description": "MAKE FACEBOOK MORE EXCITING!",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "http://www.facebook.com/*", "https://www.facebook.com/*"
  ],
	"content_scripts": [
		{
			"matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
			"css": ["zomgbook.css"],
			"js": ["zomgbook.js"]
		}
  ],
  "web_accessible_resources": [
    "images/1.jpg",
    "images/2.jpg",
    "images/3.jpg",
    "images/4.jpg",
    "images/5.jpg",
    "images/6.jpg",
    "images/7.jpg",
    "images/8.jpg",
    "images/9.jpg",
    "images/10.jpg"
  ]
}
